

/* se um minuto tem 60 segundos, quantos segundos existem em X minutos? Escreva um programa que receba a quantidade de minutos e imprima quantos segundos existem*/


#include <stdio.h>

int main()

{
  int minutos, segundos;
  printf("Digite os minutos: ");
  scanf("%d", &minutos);
  segundos = 60 * minutos;
  printf("%d\n", segundos);
  
  return 0;
  
}

